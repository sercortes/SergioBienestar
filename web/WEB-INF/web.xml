<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>co.edu.sena.bienestar.sergio.controller.login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Routes</servlet-name>
        <servlet-class>co.edu.sena.bienestar.sergio.controller.Routes</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>activitys</servlet-name>
        <servlet-class>co.edu.sena.bienestar.sergio.controller.activitys</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>aprendiz</servlet-name>
        <servlet-class>co.edu.sena.bienestar.sergio.controller.aprendiz</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Select</servlet-name>
        <servlet-class>co.edu.sena.bienestar.sergio.controller.Select</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UploadFile</servlet-name>
        <servlet-class>co.edu.sena.bienestar.sergio.controller.UploadFile</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SelectQuerys</servlet-name>
        <servlet-class>co.edu.sena.bienestar.sergio.controller.SelectQuerys</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Responsable</servlet-name>
        <servlet-class>co.edu.sena.bienestar.sergio.controller.Responsable</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>User</servlet-name>
        <servlet-class>co.edu.sena.bienestar.sergio.controller.User</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EventsLogs</servlet-name>
        <servlet-class>co.edu.sena.bienestar.sergio.controller.EventsLogs</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Routes</servlet-name>
        <url-pattern>/Start</url-pattern>
        <url-pattern>/Logout</url-pattern>
        <url-pattern>/Home</url-pattern>
        <url-pattern>/Upload</url-pattern>
        <url-pattern>/Activitys</url-pattern>
        <url-pattern>/Aprendices</url-pattern>
        <url-pattern>/Statistics</url-pattern>
        <url-pattern>/StatisticsDimensions</url-pattern>
        <url-pattern>/Responsables</url-pattern>
        <url-pattern>/Users</url-pattern>
        <url-pattern>/Profile</url-pattern>
        <url-pattern>/Logs</url-pattern>
        <url-pattern>/Error404</url-pattern>
        <url-pattern>/Error500</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>activitys</servlet-name>
        <url-pattern>/ListActivitysSearch</url-pattern>
        <url-pattern>/ListActivitysByAprendiz</url-pattern>
        <url-pattern>/ListStaticsBytype</url-pattern>
        <url-pattern>/ListStaticsBytypeFicha</url-pattern>
        <url-pattern>/ListStaticsBytypeProgram</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>aprendiz</servlet-name>
        <url-pattern>/ListAprendices</url-pattern>
        <url-pattern>/getByCoor</url-pattern>
        <url-pattern>/getTypesActivitys</url-pattern>
        <url-pattern>/ListActivitysByType</url-pattern>
        <url-pattern>/ListAprendicesByFicha</url-pattern>
        <url-pattern>/ListAprendicesByCoor</url-pattern>
        <url-pattern>/getSearchAprendiz</url-pattern>
        <url-pattern>/getSearchAprendizWord</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Select</servlet-name>
        <url-pattern>/getFichas</url-pattern>
        <url-pattern>/getCoor</url-pattern>
        <url-pattern>/getYears</url-pattern>
        <url-pattern>/getPerfil</url-pattern>
        <url-pattern>/getUser</url-pattern>
        <url-pattern>/getPrograms</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UploadFile</servlet-name>
        <url-pattern>/Uploading</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SelectQuerys</servlet-name>
        <url-pattern>/SelectYears</url-pattern>
        <url-pattern>/SelectByTypesbyYear</url-pattern>
        <url-pattern>/SelectStatiticsByActivity</url-pattern>
        <url-pattern>/SelectgetProgramsWithOutCoor</url-pattern>
        <url-pattern>/UpdateProgram</url-pattern>
        <url-pattern>/SelectByTypesbyYearProgram</url-pattern>
        <url-pattern>/SelectByTypesbyYearProgramGeneral</url-pattern>
        <url-pattern>/SelectDimensions</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Responsable</servlet-name>
        <url-pattern>/SelectResponsables</url-pattern>
        <url-pattern>/addResponsables</url-pattern>
        <url-pattern>/delResponsables</url-pattern>
        <url-pattern>/editResponsables</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>User</servlet-name>
        <url-pattern>/SelectUsers</url-pattern>
        <url-pattern>/addUser</url-pattern>
        <url-pattern>/deluser</url-pattern>
        <url-pattern>/editUser</url-pattern>
        <url-pattern>/updatePass</url-pattern>
        <url-pattern>/updatePassIndividual</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EventsLogs</servlet-name>
        <url-pattern>/getEventsLogs</url-pattern>
    </servlet-mapping>
    <error-page>
        <error-code>404</error-code>
        <location>/Error404</location>
    </error-page>
    <error-page>
        <error-code>500</error-code>
        <location>/Error500</location>
    </error-page>
    <error-page>
        <error-code>405</error-code>
        <location>/Error500</location>
    </error-page>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
