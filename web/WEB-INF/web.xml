<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>co.edu.sena.bienestar.sergio.controller.login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>test</servlet-name>
        <servlet-class>co.edu.sena.bienestar.sergio.controller.test</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Routes</servlet-name>
        <servlet-class>co.edu.sena.bienestar.sergio.controller.Routes</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>activitys</servlet-name>
        <servlet-class>co.edu.sena.bienestar.sergio.controller.activitys</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>aprendiz</servlet-name>
        <servlet-class>co.edu.sena.bienestar.sergio.controller.aprendiz</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>test</servlet-name>
        <url-pattern>/Uploading</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Routes</servlet-name>
        <url-pattern>/Home</url-pattern>
        <url-pattern>/Upload</url-pattern>
        <url-pattern>/Activitys</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>activitys</servlet-name>
        <url-pattern>/ListActivitys</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>aprendiz</servlet-name>
        <url-pattern>/ListAprendices</url-pattern>
        <url-pattern>/getPrograms</url-pattern>
        <url-pattern>/getFichas</url-pattern>
        <url-pattern>/getCoor</url-pattern>
        <url-pattern>/getByCoor</url-pattern>
        <url-pattern>/getTypesActivitys</url-pattern>
        <url-pattern>/ListActivitysByType</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
